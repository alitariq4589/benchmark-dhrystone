node ('sf1-1'){
    stage ('*** Cleanup sf1-1 ***'){
        cleanWs()
    }
    stage('*** Checkout SCM sf1-1 ***'){
      sh"""#!/bin/bash
        source ~/.bash_aliases
        set +e
        git_clone_func https://github.com/alitariq4589/benchmark-dhrystone.git
        set -e
      """
    }
    stage ('*** make sf1-1 ***'){
        sh'''#!/bin/bash
            make CC=gcc
        '''
    }
    stage ('*** Run sf1-1 ***'){
        sh'''#!/bin/bash
            ./dhrystone
        '''
    }
    stage ('Cleanup sf1-1'){
        cleanWs()
    }
}

