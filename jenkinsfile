node ('qemuusermode_runner2'){
    stage ('*** Cleanup ***'){
        cleanWs()
    }
    stage ('*** Check out SCM ***'){
        checkout scm
    }
    stage ('*** make ***'){
        sh'''#!/bin/bash
            make CC=riscv64-unknown-linux-gnu-gcc
        '''
    }
    stage ('*** Run ***'){
        sh'''#!/bin/bash
            qemu-riscv64 -L $RISCV_SYSROOT ./dhrystone
        '''
    }
}
node ('riscv64_runner1'){
    stage ('*** Cleanup ***'){
        cleanWs()
    }
    stage ('*** Check out SCM ***'){
        checkout scm
    }
    stage ('*** make ***'){
        sh'''#!/bin/bash
            make
        '''
    }
    stage ('*** Run ***'){
        sh'''#!/bin/bash
            ./dhrystone
        '''
    }
}
